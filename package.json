{
  "name": "@veterancrowd/aws-service-config",
  "version": "0.0.8",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/veterancrowd/aws-service-config"
  },
  "author": "Jason G. Williscroft",
  "bugs": {
    "url": "https://github.com/veterancrowd/aws-service-config/issues"
  },
  "description": "Abstracts service connections into a class.",
  "homepage": "https://github.com/veterancrowd/aws-service-config#readme",
  "keywords": [
    "aws"
  ],
  "license": "BSD-3-Clause",
  "dependencies": {
    "@aws-sdk/client-api-gateway": "^3.369.0",
    "ajv": "^8.12.0",
    "aws-xray-sdk": "^3.5.0",
    "exponential-backoff": "^3.1.1",
    "http-errors": "^2.0.0",
    "lodash": "^4.17.21",
    "openapi-client-axios": "^7.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/eslint-parser": "^7.22.9",
    "@babel/plugin-syntax-import-assertions": "^7.22.5",
    "@babel/preset-env": "^7.22.9",
    "@babel/register": "^7.22.5",
    "@types/node": "^20.4.2",
    "@veterancrowd/aws-cli": "^4.3.1",
    "chai": "^4.3.7",
    "concat-md": "^0.5.1",
    "crlf": "^1.1.1",
    "eslint": "^8.44.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^46.4.3",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-yml": "^1.8.0",
    "jsdoc-to-markdown": "^8.0.0",
    "lefthook": "^1.4.6",
    "mocha": "^10.2.0",
    "release-it": "^16.1.0"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js"
    }
  },
  "main": "./lib/index.js",
  "mocha": {
    "exclude": [
      "./node_modules/**"
    ],
    "file": "./test/setup.js",
    "require": [
      "@babel/register"
    ],
    "spec": "./**/*.test.!(*.*)",
    "timeout": 10000
  },
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": true
    }
  },
  "scripts": {
    "doc": "jsdoc2md -c doc/jsdoc.config.json -f lib/**/*.* -t doc/api-template.hbs > doc/3-api.jsdoc2.md && concat-md doc --hide-anchor-links > README.md && crlf --set=CRLF README.md",
    "lint": "eslint lib/**",
    "prerelease": "npm run lint && npm run test && npm run doc",
    "release": "release-it",
    "test": "vc -c \"mocha\""
  },
  "type": "module"
}
