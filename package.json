{
  "name": "@veterancrowd/aws-service-config",
  "version": "0.2.17",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/veterancrowd/aws-service-config"
  },
  "author": "Jason G. Williscroft",
  "bugs": {
    "url": "https://github.com/veterancrowd/aws-service-config/issues"
  },
  "description": "Abstracts service connections into a class.",
  "homepage": "https://github.com/veterancrowd/aws-service-config#readme",
  "keywords": [
    "aws"
  ],
  "license": "BSD-3-Clause",
  "dependencies": {
    "@aws-sdk/client-api-gateway": "^3.511.0",
    "ajv": "^8.12.0",
    "exponential-backoff": "^3.1.1",
    "http-errors": "^2.0.0",
    "lodash": "^4.17.21",
    "openapi-client-axios": "^7.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/plugin-syntax-import-assertions": "^7.23.3",
    "@babel/preset-env": "^7.23.9",
    "@babel/register": "^7.23.7",
    "@types/node": "^20.11.17",
    "@veterancrowd/aws-cli": "^4.6.2",
    "auto-changelog": "^2.4.0",
    "chai": "^5.1.0",
    "concat-md": "^0.5.1",
    "crlf": "^1.1.1",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.0.6",
    "eslint-plugin-mocha": "^10.2.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-yml": "^1.12.2",
    "jsdoc-to-markdown": "^8.0.1",
    "lefthook": "^1.6.1",
    "mocha": "^10.3.0",
    "release-it": "^17.0.3"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js"
    }
  },
  "main": "./lib/index.js",
  "mocha": {
    "exclude": [
      "./node_modules/**"
    ],
    "file": "./test/setup.js",
    "require": [
      "@babel/register"
    ],
    "spec": "./**/*.test.!(*.*)",
    "timeout": 10000
  },
  "release-it": {
    "git": {
      "changelog": "npx auto-changelog --stdout --commit-limit false -u --template https://raw.githubusercontent.com/release-it/release-it/main/templates/changelog-compact.hbs",
      "requireBranch": "dev",
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    },
    "hooks": {
      "after:init": [
        "nr lint",
        "nr test",
        "nr doc"
      ],
      "after:release": [
        "git switch -c release/${version}",
        "git push -u origin release/${version}",
        "git switch dev"
      ]
    },
    "npm": {
      "publish": true
    }
  },
  "scripts": {
    "doc": "jsdoc2md -c doc/jsdoc.config.json -f lib/**/*.* -t doc/api-template.hbs > doc/3-api.jsdoc2.md && concat-md doc --hide-anchor-links > README.md && crlf --set=CRLF README.md",
    "lint": "eslint lib/**",
    "release": "vc -c \"release-it\"",
    "test": "vc -c \"mocha\""
  },
  "type": "module"
}
